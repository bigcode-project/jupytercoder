{
  "manifest_version": 3,
  "name": "Jupyter Coder",
  "version": "0.1.0.1",
  "description": "Capture data in Jupyter Notebook cells and send to a remote server.",
  "icons": {
    "32": "icons/flow_64.png",
    "48": "icons/flow_128.png",
    "128": "icons/flow_256.png"
  },
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "options.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ]
}
